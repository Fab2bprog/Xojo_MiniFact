	#tag Method, Flags = &h0
		Function Touche_Valide(key as String) As Boolean
		  if key=chr(13)  then return true
		  if key=chr(3)    then return true
		  
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Function Valide_Formulaire() As Boolean
		  Dim result as Variant
		  Dim TitreMsg as String
		  Dim IconeMsg as Integer
		  
		  TitreMsg="REFUS DE PRISE EN COMPTE DU MOT DE PASSE"
		  IconeMsg=48
		  
		  if Txt_AncPassword.Text<> AncPassword  then
		    result=MsgBox ("Le mot de passe original que vous avez saisie est faut ! ",IconeMsg,TitreMsg)
		    return false
		  end if
		  
		  if Txt_NvxPassword1.Text="" or Txt_NvxPassword2.Text=""  then
		    result=MsgBox ("Les deux champs relatifs au nouveau mot de passe doivent obligatoirement etre complété !",IconeMsg,TitreMsg)
		    return false
		  end if
		  
		  if Txt_NvxPassword1.Text <> Txt_NvxPassword2.Text  then
		    result=MsgBox ("Le nouveau mot de passe et sa confirmation ne correspondent pas : vous avez mal saisie votre nouveau mot de passe  !",IconeMsg,TitreMsg)
		    return false
		  end if
		  
		  if Txt_AncPassword.Text = Txt_NvxPassword1.Text   then
		    result=MsgBox ("L'ancien mot de passe est identique au nouveau !",IconeMsg,TitreMsg)
		    return false
		  end if
		  
		  if len(Txt_NvxPassword1.Text)<3   then
		    result=MsgBox ("Le mot de passe doit avoir 3 caracteres de long au minimum et 20 au maximum !",IconeMsg,TitreMsg)
		    return false
		  end if
		  
		  
		  return true
		  
		End Function
